{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": "off",
        "no-unused-vars": "off",
        "indent": [
            "error",
            4
        ]
    }
}
